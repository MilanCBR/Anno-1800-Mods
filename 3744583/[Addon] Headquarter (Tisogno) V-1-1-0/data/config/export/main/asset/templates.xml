	<ModOp Path="//Template[Name='Guildhouse']" Type="addNextSibling">
		<Template>
			<Name>HQ_Palace</Name>
			<Properties>
          			<Standard />
          			<Building>
            				<BuildingType>Factory</BuildingType>
    					<BuildingCategoryName>11152</BuildingCategoryName>
    					<BuildModeRandomRotation>90</BuildModeRandomRotation>
    					<AssociatedRegions>Moderate</AssociatedRegions>
          			</Building>
          			<Blocking />
          			<Cost>
            				<InfluenceCostType>Townhall</InfluenceCostType>
          			</Cost>
          			<Object>
            				<IgnoreForMinimapGeneration>1</IgnoreForMinimapGeneration>
          			</Object>
  				<Mesh>
    					<LogoMode>CorporationLogoFromPlayer</LogoMode>
    					<SetCompanyLogo>1</SetCompanyLogo>
  				</Mesh>
          			<Selection>
            				<GUIType>Guildhouse</GUIType>
            				<OnSelection>
              					<IsBaseAutoCreateAsset>1</IsBaseAutoCreateAsset>
              					<Values>
                					<ActionList />
              					</Values>
            				</OnSelection>
    					<Colors>
      						<WeakSelectionColorType>NoColor</WeakSelectionColorType>
    					</Colors>
            				<AllowedDragType>ItemSocket;CargoItemStack;StorageItemStack;SharedTransferItem</AllowedDragType>
          			</Selection>
          			<Text />
          			<Constructable>
            				<DestructionCategory>Production</DestructionCategory>
          			</Constructable>
          			<Locked />
          			<SoundEmitter>
            				<DestroySounds>
              					<Item>
                					<Sound>9818756</Sound>
              					</Item>
            				</DestroySounds>
          			</SoundEmitter>
  				<AmbientMoodProvider />
          			<FeedbackController />
          			<Infolayer />
          			<ItemContainer>
                          		<SocketCount>3</SocketCount>	
                          		<SocketAllocation>Headquarter</SocketAllocation>
 					<SocketScope>Area</SocketScope>
     					<SocketScopeRadiusOverlapCategory>None</SocketScopeRadiusOverlapCategory>  
          			</ItemContainer>
          			<UpgradeList />
          			<Attackable>
            				<HasRuinState>1</HasRuinState>
            				<ExplosionEffects>
              					<Item />
            				</ExplosionEffects>
          			</Attackable>
          			<Pausable />
          			<Maintenance>
            				<IncomeCategory>AdministrationBuildings</IncomeCategory>
          			</Maintenance>
          			<IncidentInfluencer>
            				<Influence>
              					<Fire>
                					<Distance>15</Distance>
              					</Fire>
              					<Riot>
                					<Distance>15</Distance>
              					</Riot>
              					<Illness>
                					<Distance>15</Distance>
              					</Illness>
              					<Explosion>
                					<Distance>15</Distance>
              					</Explosion>
            				</Influence>
          			</IncidentInfluencer>
  				<Culture>
    					<CultureType>Culture</CultureType>
       				</Culture>
  				<InfluenceSource />
				<PublicService />		<!-- necessary: otherwise BusActivation would not work -->
				<StreetActivation />
				<BusActivation />
          			<BuildingUnique />
			</Properties>
      		</Template>
	</ModOp>

